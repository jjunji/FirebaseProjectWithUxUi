                android:id="@+id/Layout_timeNow">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="36dp"
                    android:layout_height="wrap_content"
                    android:text="30C" />

                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="36dp"
                    android:layout_height="36dp"
                    app:srcCompat="@drawable/sun" />
            </LinearLayout>

            <TextView
