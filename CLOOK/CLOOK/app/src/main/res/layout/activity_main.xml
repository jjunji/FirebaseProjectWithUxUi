                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp"
                android:text="시간대별 날씨"
                android:textSize="14dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="16dp" />

            <LinearLayout
                android:layout_width="36dp"
                android:layout_height="80dp"
                android:layout_marginBottom="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="12dp"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toRightOf="@+id/Layout_timeNow"
                app:layout_constraintTop_toBottomOf="@+id/textView7"
                app:layout_constraintVertical_bias="0.6"
                android:layout_marginStart="16dp"
                android:id="@+id/Layout_time12"></LinearLayout>

            <LinearLayout
                android:layout_width="36dp"
                android:layout_height="80dp"
                android:layout_marginTop="12dp"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="16dp"
                app:layout_constraintTop_toBottomOf="@+id/textView7"
                app:layout_constraintLeft_toRightOf="@+id/Layout_time12"
                android:layout_marginLeft="16dp"
                android:layout_marginStart="16dp"
                android:id="@+id/Layout_time15"></LinearLayout>

            <LinearLayout
                android:layout_width="28dp"
                android:layout_height="105dp"
                android:orientation="vertical"
                tools:layout_editor_absoluteX="689dp"
                tools:layout_editor_absoluteY="1719dp"></LinearLayout>

            <LinearLayout
                android:layout_width="36dp"
                android:layout_height="80dp"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginTop="12dp"
                app:layout_constraintTop_toBottomOf="@+id/textView7"
                android:layout_marginBottom="16dp"
                app:layout_constraintLeft_toRightOf="@+id/Layout_time18"
                android:layout_marginLeft="16dp"
                android:layout_marginStart="12dp"
                android:id="@+id/Layout_time21"></LinearLayout>

            <LinearLayout
                android:id="@+id/Layout_time0"
                android:layout_width="36dp"
                android:layout_height="80dp"
                android:layout_marginBottom="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="12dp"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintLeft_toRightOf="@+id/Layout_time21"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7"></LinearLayout>
        </android.support.constraint.ConstraintLayout>

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
